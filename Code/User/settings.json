// Place your settings in this file to overwrite the default settings
{
  "amVim.startInInsertMode": true,
  "amVim.mimicVimSearchBehavior": false,
  "bracket-pair-colorizer-2.colors": [
    "#ff6188",
    "#fc9867",
    "#ffd866",
    "#a9dc76",
    "#78dce8",
    "#ab9df2"
  ],
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "bracket-pair-colorizer-2.showVerticalScopeLine": false,
  "bracket-pair-colorizer-2.unmatchedScopeColor": "#da3c50",
  "breadcrumbs.enabled": true,
  "cSpell.allowCompoundWords": true,
  "editor.autoIndent": "full",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Office Code Pro",
  "editor.fontSize": 15,
  "editor.formatOnType": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    120
  ],
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.countBadge": "off",
  "html.format.indentHandlebars": true,
  "html.format.wrapLineLength": 0,
  "java.semanticHighlighting.enabled": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "javascript.referencesCodeLens.enabled": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "python.insidersChannel": "off",
  "python.pythonPath": "/usr/local/bin/python3",
  "rewrap.autoWrap.enabled": false,
  "shellcheck.enableQuickFix": true,
  "terminal.integrated.fontSize": 14,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.restoreWindows": "none",
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editor.findMatchBackground": "#383e6b",
    "editor.findMatchHighlightBackground": "#383e6b",
    "editorGroupHeader.tabsBackground": "#221f22",
    "editorOverviewRuler.findMatchForeground": "#7a87e6",
    "editorOverviewRuler.selectionHighlightForeground": "#bebebd",
    "editorOverviewRuler.wordHighlightForeground": "#bebebd",
    "editorOverviewRuler.wordHighlightStrongForeground": "#bebebd",
    "tab.border": "#221f22",
    "tab.inactiveBackground": "#221f22"
  },
  "workbench.colorTheme": "Monokai Pro",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.startupEditor": "newUntitledFile",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "extensions.ignoreRecommendations": false,
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "files.associations": {
    "*.pom": "xml"
  },
  "cSpell.enabled": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter-notebook",
      "filenamePattern": "*.ipynb"
    }
  ],
  "workbench.editor.untitled.hint": "hidden"
}
