// Place your settings in this file to overwrite the default settings
{
  "amVim.startInInsertMode": true,
  "amVim.mimicVimSearchBehavior": false,
  "amVim.useSystemClipboard": true,
  "bracketLens.color": "#99999959",
  "bracketLens.mode": "auto",
  "breadcrumbs.enabled": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.enabled": true,
  "editor.autoIndent": "full",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Office Code Pro",
  "editor.fontSize": 15,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.inlineSuggest.enabled": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    120
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "extensions.supportUntrustedWorkspaces": {
    "auiworks.amvim": {
      "supported": true
    },
  },
  "files.associations": {
    "*.pom": "xml"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.countBadge": "off",
  "gitlens.advanced.abbreviateShaOnCopy": true,
  "go.toolsManagement.autoUpdate": true,
  "html.format.indentHandlebars": true,
  "html.format.wrapLineLength": 0,
  "js/ts.implicitProjectConfig.checkJs": true,
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.insidersChannel": "weekly",
  "rewrap.autoWrap.enabled": false,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "newWindow",
  "shellcheck.enableQuickFix": true,
  "terminal.integrated.fontSize": 15,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.restoreWindows": "none",
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#ff6188",
    "editorBracketHighlight.foreground2": "#fc9867",
    "editorBracketHighlight.foreground3": "#ffd866",
    "editorBracketHighlight.foreground4": "#a9dc76",
    "editorBracketHighlight.foreground5": "#78dce8",
    "editorBracketHighlight.foreground6": "#ab9df2",
    "editorBracketHighlight.unexpectedBracket.foreground": "#da3c50",
    "editor.findMatchBackground": "#383e6b",
    "editor.findMatchHighlightBackground": "#383e6b",
    "editorGroupHeader.tabsBackground": "#221f22",
    "editorOverviewRuler.findMatchForeground": "#7a87e6",
    "editorOverviewRuler.selectionHighlightForeground": "#bebebd",
    "editorOverviewRuler.wordHighlightForeground": "#bebebd",
    "editorOverviewRuler.wordHighlightStrongForeground": "#bebebd",
    "tab.border": "#221f22",
    "tab.inactiveBackground": "#221f22"
  },
  "workbench.colorTheme": "Monokai Pro",
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.startupEditor": "newUntitledFile"
}
